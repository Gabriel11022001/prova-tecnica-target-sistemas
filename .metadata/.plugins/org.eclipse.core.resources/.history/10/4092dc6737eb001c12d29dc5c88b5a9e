		List<Faturamento> faturamentos = new ArrayList<Faturamento>();
	}
	public static void main(String[] args) {
		try {
			List<Faturamento> faturamentos = lerDados();
			
		} catch (Exception e) {
			System.out.println(e.getMessage());
		}
	}
}
